{
    "name": "electrolux-web-app",
    "version": "3.1.0",
    "description": "Web App for Electrolux. Version @2.0.0 was built entirely on SOLID principles. Version @2.1.0, besides checking and upgrading dependencies, uses Singleton's Desing Pattern on some of its classes. Version @3.1.0 is completely refactored and has now a more controled flow, considering performance gains, and makes use of a more robust architecture",
    "main": "/info/ts/main.ts",
    "private": true,
    "type": "module",
    "author": "Thiago Bora",
    "scripts": {
        "prettier": "prettier . !./info/**/*.html --write",
        "fix": "prettier . !./info/**/*.html --write && eslint --fix .",
        "fix:all": "prettier . --write && eslint --fix",
        "dev": "vite",
        "build": "concurrently \"npm run fix:all\" \"tsc && vite build --mode production --emptyOutDir\"",
        "preview": "vite preview"
    },
    "devDependencies": {
        "@eslint/eslintrc": "^3.2.0",
        "@eslint/js": "^9.19.0",
        "@types/node": "^22.10.10",
        "@typescript-eslint/eslint-plugin": "^8.23.0",
        "@typescript-eslint/parser": "^8.23.0",
        "@vitejs/plugin-legacy": "^6.0.2",
        "concurrently": "^9.1.2",
        "eslint": "^9.19.0",
        "eslint-config-prettier": "^10.0.1",
        "eslint-plugin-prettier": "^5.2.3",
        "globals": "^15.14.0",
        "prettier": "^3.4.2",
        "typescript": "^5.7.3",
        "typescript-eslint": "^8.23.0",
        "vite": "^6.0.5",
        "vite-plugin-eslint": "^1.8.1",
        "vite-plugin-html": "^3.2.2",
        "vite-plugin-static-copy": "^2.2.0"
    },
    "dependencies": {
        "axios": "^1.8.4",
        "pino": "^9.6.0",
        "swiper": "^11.2.1",
        "zod": "^3.24.2"
    }
}
